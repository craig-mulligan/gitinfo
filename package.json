{
  "name": "gitinfo",
  "description": "Get info about a local clone of a GitHub repository.",
  "keywords": [
    "github",
    "repository",
    "info"
  ],
  "version": "0.0.0-semantic-release",
  "main": "./dist/gitinfo.js",
  "author": {
    "name": "Gajus Kuizinas",
    "email": "gajus@gajus.com",
    "url": "http://gajus.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/gitinfo"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --copy-files",
    "test": "NODE_ENV=development mocha --compilers js:babel-register ./tests",
    "lint": "eslint ./src ./tests"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-register": "^6.11.6",
    "chai": "^3.3.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.2.2",
    "eslint-config-canonical": "^1.7.12",
    "mocha": "^3.0.2",
    "nyc": "^7.1.0",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "ini": "^1.3.4"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-lcov"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
